          <characteristics>
            <characteristic name="M" typeId="b935-4488-d358-6e91">0&quot;</characteristic>
            <characteristic name="A" typeId="d2e4-6a9a-b9b6-4e73">3</characteristic>
            <characteristic name="Acc" typeId="abd4-f7cd-9be6-e5cb">6+</characteristic>
            <characteristic name="W" typeId="4423-cb9e-dcdc-ff3d">3</characteristic>
            <characteristic name="Re" typeId="a49f-fcd4-4203-ab27">5+</characteristic>
